{
  "name": "fastify-mongoose",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "files": [
    "build",
    "index.js"
  ],
  "scripts": {
    "build": "babel src -d build",
    "test:build": "babel tests -d test",
    "dev": "npm run build && node ./index.js",
    "lint": "eslint --ignore-path .gitignore ./src ./tests",
    "test": "npm run test:build && npm run test:run",
    "test:run": "./node_modules/.bin/mocha --require ./test/bootstrap.js --timeout 10000 --recursive   ./test/**/*.test.js",
    "coverage": "npm run test:build && nyc --reporter html --reporter text npm test",
    "pre:commit": "npm run lint && npm run coverage --silent",
    "pre:publish": "npm run lint && npm run build && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/sonsor/fastify-mongoose.git"
  },
