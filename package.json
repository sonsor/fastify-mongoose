    "url": "https://gitlab.com/sonsor/fastify-mongoose/issues"
  },
  "homepage": "https://gitlab.com/sonsor/fastify-mongoose#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.7.1",
    "eslint-config-node": "^4.0.0",
    "husky": "^3.1.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0"
  },
